{
    "buildCommand": "",
    "outputDirectory": "backend/static",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://YOUR_RAILWAY_BACKEND_URL/:path*"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}